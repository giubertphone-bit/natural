---
import Layout from "../layouts/Layout.astro";
import CtaTreeService from "../components/ctaTreeService.astro";
import LawnService from "../components/LawnService.astro";
import GrassDivider from "../components/grassDivider.astro";
---

<Layout>
  <div class="ctaBox">
    <CtaTreeService />
  </div>
  <LawnService />
  <GrassDivider />
</Layout>

<style>
  #testImg {
    width: 100%;
    height: auto;
    display: block;
    margin: 0 auto;
  }
  .ctaBox {
    max-width: 1200px;
    margin: 20px;
    padding: 2px;
    box-shadow: 0px 0px 3px rgba(0, 0, 0, 0.7);
  }
</style>
